env:
  browser: true
  jquery: true
extends: 'eslint:recommended'
rules:
  array-bracket-spacing:
    - error
    - never
  object-curly-spacing:
    - error
    - never
  indent:
    - error
    - 2
  linebreak-style:
    - error
    - unix
  quotes:
    - error
    - single
  semi:
    - error
    - always
  space-infix-ops:
    - error
